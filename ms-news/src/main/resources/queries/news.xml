<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>News Queries</comment>

    <entry key="news.query.create">
        <![CDATA[
            INSERT
                INTO news
            VALUES(NEWS_SEQ.nextval,
                  :header,
                  :body,
                  :type,
                  :pictureKey,
                  :lastModified);
        ]]>
    </entry>

    <entry key="news.query.lastAdded">
        <![CDATA[
            SELECT
                MAX(id)
            FROM news;
        ]]>
    </entry>

    <entry key="bin.query.findAll">
        <![CDATA[

        ]]>
    </entry>

</properties>